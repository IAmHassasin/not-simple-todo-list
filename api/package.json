    "prisma:seed": "yarn dev yarn ts-node prisma/seed/seed.ts"
  },
  "devDependencies": {
    "prisma": "^6.4.1"
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "@types/node": "*",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
